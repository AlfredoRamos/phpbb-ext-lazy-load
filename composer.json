{
	"name": "alfredoramos/lazyloading",
	"type": "phpbb-extension",
	"description": "Lazy Loading Extension for phpBB 3.2.x",
	"homepage": "https://github.com/AlfredoRamos/phpbb-ext-lazy-loading",
	"version": "0.1.0-alpha",
	"time": "2018-10-09",
	"keywords": ["phpbb", "extension", "lazy-loading"],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "Alfredo Ramos",
			"email": "alfredo.ramos@yandex.com",
			"homepage": "https://github.com/AlfredoRamos",
			"role": "Lead Developer"
		}
	],
	"require": {
		"php": ">=5.6",
		"composer/installers": "^1.6.0"
	},
	"require-dev": {
		"phing/phing": "^2.16.0",
		"pear/archive_tar": "^1.4.0"
	},
	"extra": {
		"display-name": "Lazy Loading",
		"soft-require": {
			"phpbb/phpbb": "~3.2.0"
		},
		"version-check": {
			"host": "alfredoramos.github.io",
			"directory": "/phpbb-extensions",
			"filename": "lazy-loading.json",
			"ssl": true
		}
	}
}
